<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.monitoreo">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Monitoreo"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.ModificarHijo"></activity>
        <activity android:name=".ui.ImagenAlerta" />
        <activity android:name=".ui.HijoInformacion" />
        <activity android:name=".ui.ModificarDatos" />
        <activity
            android:name=".ui.Menu"
            android:label="Activity Title"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.AgregarHijo" />
        <activity android:name=".ui.RecuperarContra" />
        <activity android:name=".ui.Registrar" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>